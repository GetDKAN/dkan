      "widget": "dkan_uuid"
    }
  },
  "contactPoint": {
    "properties": {
      "@type": {
        "ui:options": {
          "widget": "hidden"
        }
      }
    }
  },
  "accessLevel": {
    "ui:options": {
      "description": "This refers to how the data can be accessed."
    }
  },
  "spatial": {
    "ui:options": {
      "widget": "textarea",
      "rows": 5,
      "title": "Relevant Location",
      "description": "If your dataset has a spatial component, please provide location such as place name or latitude/longitude pairs."
    }
  },
  "isPartOf": {
    "ui:options": {
      "widget": "hidden"
    }
  },
  "publisher": {
    "ui:options": {
      "widget": "list",
      "type": "autocomplete",
      "allowCreate": "true",
      "titleProperty": "name",
      "source": {
        "metastoreSchema": "publisher"
      }
    },
    "properties": {
      "@type": {
        "ui:options": {
          "widget": "hidden"
        }
      },
      "subOrganizationOf": {
        "ui:options": {
          "widget": "hidden"
        }
      }
    }
  },
  "license": {
    "ui:options": {
      "widget": "list",
      "type": "select_other",
      "other_type": "url",
      "source": {
        "enum": [
          "https://creativecommons.org/licenses/by/4.0/",
          "https://creativecommons.org/licenses/by-sa/4.0/",
          "https://creativecommons.org/publicdomain/zero/1.0/",
          "https://creativecommons.org/licenses/by-nc/2.5/",
          "https://creativecommons.org/licenses/by-nc-nd/4.0/",
