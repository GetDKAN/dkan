        "enumNames": [
          "Creative Commons Attribution",
          "Creative Commons Attribution Share-Alike",
          "Creative Commons CCZero",
          "Creative Commons Non-Commercial (2.5)",
          "Attribution NonCommercial NoDerivatives 4.0 International",
          "GNU Free Documentation License",
          "Open Data Commons Attribution License",
          "Open Data Commons Open Database License (ODbL)",
          "Open Data Commons Public Domain Dedication and Licence (PDDL)",
          "UK Open Government Licence (OGL)"
        ]
      }
    }
  },
  "distribution": {
    "items": {
      "@type": {
        "ui:options": {
          "widget": "hidden"
        }
      },
      "mediaType": {
        "ui:options": {
          "widget": "hidden"
        }
      },
      "description": {
        "ui:options": {
          "widget": "textarea",
          "rows": 5
        }
      },
      "format": {
        "ui:options": {
          "widget": "list",
          "type": "select_other",
          "other_type": "textfield",
          "source": {
            "enum": [
              "arcgis",
              "csv",
              "esri rest",
              "geojson",
              "json",
              "kml",
              "pdf",
              "tsv",
              "xls",
              "xlsx",
              "xml",
              "zip"
            ]
          }
        }
      },
      "downloadURL": {
        "ui:options": {
          "widget": "upload_or_link",
          "extensions": "csv html xls json xlsx doc docx rdf txt jpg png gif tiff pdf odf ods odt tsv tab geojson xml zip kml kmz shp",
          "progress_indicator": "bar"
        }
      }
    }
  },
  "description": {
    "ui:options": {
      "widget": "textarea",
      "rows": 5
    }
  },
  "references": {
    "items": {
      "ui:options": {
        "placeholder": "http://"
      }
    }
  },
  "issued": {
    "ui:options": {
      "widget": "flexible_datetime"
    }
  },
  "modified": {
    "ui:options": {
      "widget": "flexible_datetime",
      "timeRequired": false,
      "placeholder": "YYYY-MM-DD"
    }
  },
  "describedBy": {
    "ui:options": {
      "placeholder": "http://"
    }
  },
  "theme": {
    "ui:options": {
      "title": "Topics",
