    "items": {
      "ui:options": {
        "title": "Topic",
        "widget": "list",
        "type": "autocomplete",
        "allowCreate": "true",
        "multiple": "true",
        "source": {
          "metastoreSchema": "theme"
        }
      }
    }
  },
  "keyword": {
    "ui:options": {
      "hideActions": "true",
      "child": "keyword"
    },
    "items": {
      "ui:options": {
        "widget": "list",
        "type": "autocomplete",
        "allowCreate": "true",
        "multiple": "true",
        "source": {
