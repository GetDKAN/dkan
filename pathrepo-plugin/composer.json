{
    "name": "getdkan/pathrepo-plugin",
    "description": "Add and remove path repositories in your project.",
    "type": "composer-plugin",
    "require": {
        "php": ">=7.3.0",
        "composer-plugin-api": "^1 || ^2"
    },
    "autoload": {
        "psr-4": {
            "Dkan\\Composer\\Plugin\\Pathrepo\\": "/src"
        }
    },
    "extra": {
        "class": "Dkan\\Composer\\Plugin\\Pathrepo\\Plugin",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "composer/composer": "^2.2@stable"
    }
}
