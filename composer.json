{
    "name": "getdkan/dkan2",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "description": "DKAN Open Data Catalog",
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.5.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "drupal/core": "~8.0",
        "drupal/entity": "1.0-beta3",
        "drupal/config_update": "1.x-dev",
        "bower-asset/json-forms": "1.6.3",
        "drupal/group": "1.x-dev"
    },
    "require-dev": {
      "burdamagazinorg/thunder-dev-tools": "dev-master",
      "drupal/coder": "8.2",
        "drupal/devel": "1.x-dev"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "enable-asset-packagist": "Social\\Composer\\AssetPackagist::execute"
    },
    "autoload": {
        "psr-4": {
            "Social\\": "src"
        }
    },
    "extra": {
        "enable-patching": true,
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "../../../core": ["type:drupal-core"],
            "../../../libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"],
            "../../../modules/contrib/{$name}": ["type:drupal-module"],
            "../../../themes/contrib/{$name}": ["type:drupal-theme"],
            "../../../profiles/contrib/{$name}": ["type:drupal-profile"],
            "../../../../drush/contrib/{$name}": ["type:drupal-drush"]
        }
    },
    "authors": [
        {
            "name": "acouch",
            "email": "acinternets@gmail.com"
        },
        {
          "name": "dafeder",
          "email": "dafeder@gmail.com"
        }
    ]
}
