                        "settings": null
                    },
                    "css": [],
                    "extras": [],
                    "position": "0",
                    "locks": [],
                    "uuid": "f6dc22ec-3290-4c94-a393-269827d3ca67",
                    "fpp_data": {
                        "title": "Facts:",
                        "bundle": "text",
                        "link": "0",
                        "path": "",
                        "reusable": "0",
                        "admin_title": "",
                        "admin_description": "",
                        "category": "Reusable Content",
                        "view_access": null,
                        "edit_access": null,
                        "uuid": "f6dc22ec-3290-4c94-a393-269827d3ca67",
                        "language": "",
                        "field_basic_text_text": [
                            {
                                "value": "* Baby Boomers dropped from representing 56% of the workforce in 2011 to only 31% in 2015.\n* Meanwhile, Generation X (those born between 1961 and 1981) expanded from 31% to 41% of the workforce.",
                                "format": "html",
                                "safe_value": "<ul>\n<li>Baby Boomers dropped from representing 56% of the workforce in 2011 to only 31% in 2015.</li>\n<li>Meanwhile, Generation X (those born between 1961 and 1981) expanded from 31% to 41% of the workforce.</li>\n</ul>\n"
                            }
                        ],
                        "rdf_mapping": []
                    }
                },
                {
                    "panel": "quarter3",
                    "type": "custom",
                    "subtype": "custom",
                    "shown": "1",
                    "access": [],
                    "configuration": {
                        "admin_title": "",
                        "title": "Facts:",
                        "body": "* Generation Y (those born after 1981) showed the greatest leap, from representing only 10% of the workforce in 2011 to a whopping 27% in 2015.\n* As for workers over 65, their ranks dropped from 3% of the workforce in 2011 to nearly 1% in 2015.",
                        "format": "html",
                        "substitute": 1
