<?php
/**
 * @file
 * Code for DKAN Custom Solr Server.
*/

/**
 * Implements hook_disable().
 */
function dkan_acquia_search_solr_disable() {
  // Delete dkan_custom_solr server
  if ($server = dkan_acquia_search_solr_query_dkan_server('dkan_custom_solr')) {
    entity_delete('search_api_server', array_keys($server)[0]);
  }
}
