<table>
  <thead>
    <tr>
      <th>{{ header[0] }}</th>
      <th>{{ header[1] }}</th>
      <th colspan="2">{{ header[2] }}</th>
    </tr>
  </thead>
  <tbody  id="field-json-metadata-dictionary-edit-index">
    {% for row in rows %}
        {% if row.field_collection %}
        <tr>
          <td colspan="4">
            {{ row.field_collection }}
          </td>
        </tr>
        {% elseif  row.type %}
        <tr>
          <td>{{ row.type }}</td>
          <td>{{ row.description }}</td>
          <td>
          {{ row.edit_button }}
          </td>
        </tr>
      {% endif %}
    {% endfor %}
  </tbody>
</table>
