{#
/**
 * @file metastore_item.html.twig
 * Default theme implementation to present Metastore item data.
 *
 * This template is used when viewing Metastore item pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_metastore_item()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('metastore_item') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
